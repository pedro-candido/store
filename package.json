{
  "name": "store",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "expo run:android",
    "clean:ios": "cd ios && pod deintegrate && pod install && cd ..",
    "commit": "cz",
    "commitzen": "cz",
    "clean:nm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && yarn",
    "ios": "expo run:ios",
    "start": "expo start --dev-client",
    "web": "expo start --web",
    "lint": "eslint .",
    "lintFixAll": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prettierFixAll": "prettier --write 'src/**/*.{ts,tsx}'",
    "fix:lintPrettier": "yarn prettierFixAll && yarn lintFixAll",
    "testDebug": "jest -o --watch --coverage=false",
    "generateCoverage": "jest --coverage=true",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "storybook-generate": "sb-rn-get-stories"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@shopify/flash-list": "1.6.4",
    "@types/react": "^18.3.3",
    "axios": "^1.7.2",
    "expo": "~51.0.18",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.17",
    "expo-status-bar": "~1.12.1",
    "jest-styled-components": "^7.2.0",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.3.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "~0.19.10",
    "styled-components": "^6.1.11",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.7",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/stack": "^6.4.0",
    "@storybook/addon-ondevice-actions": "^7.6.20",
    "@storybook/addon-ondevice-controls": "^7.6.20",
    "@storybook/react-native": "^7.6.20",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-loader": "^9.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.x",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "prettier": "^3.3.2",
    "react-dom": "18.2.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "^18.3.1",
    "typedoc": "^0.26.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
